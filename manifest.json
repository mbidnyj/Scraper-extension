{
  "name" : "Linkout Extension",
  "version" : "0.8",
  "description" : "Linkout Extension to access cookie session",
  "permissions": [ "cookies", "tabs", "http://*.supersite.com", "https://*.linkedin.com/", "http://localhost/*", "https://stingray-app-l9lbc.ondigitalocean.app/*",  "identity", "identity.email" ],
  "icons": { "16": "linkout.png", "48": "linkout.png", "128": "linkout.png" },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": "linkout.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
